{
	"name": "@ioclub/website",
	"version": "0.1.0",
	"sideEffects": false,
	"scripts": {
		"init-db:push": "prisma db push --force-reset",
		"init-db:seed": "prisma db seed",
		"build:css": "postcss styles --base styles --dir app/styles --env production",
		"build:remix": "remix build",
		"dev:css": "postcss styles --base styles --dir app/styles -w",
		"dev:remix": "remix watch",
		"dev:run": "nodemon ./build/index.js --watch ./build/index.js",
		"init-db": "dotenv -e .env -e .env.development -- run-s init-db:*",
		"dev": "dotenv -e .env -e .env.development -- run-p dev:*",
		"build": "dotenv -e .env -e .env.production -- run-s build:*",
		"serve": "dotenv -e .env -e .env.production -- node ./build/index.js"
	},
	"dependencies": {
		"@babylonjs/core": "^5.31.0",
		"@babylonjs/gui": "^5.31.0",
		"@fastify/compress": "^6.1.1",
		"@fastify/static": "^6.5.0",
		"@mcansh/remix-fastify": "^1.12.0",
		"@prisma/client": "^4.5.0",
		"@remix-run/react": "^1.7.5",
		"@remix-run/serve": "^1.7.5",
		"@remix-run/server-runtime": "^1.7.5",
		"@unocss/preset-mini": "^0.46.3",
		"@unocss/reset": "^0.46.3",
		"cookie-signature": "^1.2.0",
		"dotenv-cli": "^6.0.0",
		"fastify": "^4.9.2",
		"fastify-plugin": "^4.3.0",
		"fastify-racing": "^1.1.0",
		"graphql": "^16.6.0",
		"katex": "^0.16.3",
		"mercurius": "^11.3.0",
		"nanoid": "^4.0.0",
		"nexus": "^1.3.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rehype-katex": "^6.0.2",
		"remark-math": "^5.1.1",
		"remix-auth": "^3.3.0",
		"remix-auth-form": "^1.1.2",
		"remix-auth-oauth2": "^1.3.0",
		"remix-utils": "^3.3.0"
	},
	"devDependencies": {
		"@iconify/json": "^2.1.134",
		"@ioclub/postcss-unocss": "workspace:^0.1.0",
		"@julr/unocss-preset-heropatterns": "^0.0.2",
		"@kidonng/daisyui": "^2.31.0",
		"@remix-run/dev": "^1.7.5",
		"@types/cookie-signature": "^1.0.4",
		"@types/node": "^18.11.9",
		"autoprefixer": "^10.4.13",
		"esbuild-register": "^3.3.3",
		"nodemon": "^2.0.20",
		"postcss": "^8.4.31",
		"postcss-cli": "^10.0.0",
		"postcss-import": "^15.0.0",
		"postcss-nesting": "^10.2.0",
		"postcss-pseudo-class-enter": "^5.0.0",
		"postcss-url": "^10.1.3",
		"prisma": "^4.5.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4",
		"unocss": "^0.46.3",
		"unocss-preset-daisy": "^1.2.0",
		"unocss-preset-scrollbar": "^0.2.1"
	},
	"prisma": {
		"seed": "node --require esbuild-register prisma/seed.ts"
	},
	"engines": {
		"node": ">=16"
	}
}
