{
	"name": "@ioclub/website",
	"version": "0.1.0",
	"sideEffects": false,
	"scripts": {
		"init-db:generate": "prisma generate",
		"init-db:push": "prisma db push --force-reset",
		"build:uno": "unocss \"app/**/*.{tsx,jsx,mdx}\" -c unocss.config.ts -o app/styles/uno.css -m",
		"build:remix": "remix build",
		"dev:uno": "unocss \"app/**/*.{tsx,jsx,mdx}\" -c unocss.config.ts -o app/styles/uno.css -w",
		"dev:remix": "remix watch",
		"dev:run": "nodemon ./build/index.js --watch ./build/index.js",
		"init-db": "dotenv -e .env -e .env.development -- run-s init-db:*",
		"dev": "dotenv -e .env -e .env.development -- run-p dev:*",
		"build": "dotenv -e .env -e .env.production -- run-s build:*",
		"serve": "dotenv -e .env -e .env.production -- node ./build/index.js"
	},
	"dependencies": {
		"@babylonjs/core": "^5.27.0",
		"@babylonjs/gui": "^5.27.0",
		"@fastify/static": "^6.5.0",
		"@mcansh/remix-fastify": "^1.12.0",
		"@prisma/client": "^4.4.0",
		"@remix-run/react": "^1.7.2",
		"@remix-run/serve": "^1.7.2",
		"@remix-run/server-runtime": "^1.7.2",
		"@unocss/preset-mini": "^0.45.29",
		"@unocss/reset": "^0.45.29",
		"cookie-signature": "^1.2.0",
		"dotenv-cli": "^6.0.0",
		"fastify": "^4.8.1",
		"fastify-plugin": "^4.2.1",
		"fastify-racing": "^1.0.3",
		"faunadb": "^4.7.0",
		"katex": "^0.16.2",
		"nanoid": "^4.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rehype-katex": "^6.0.2",
		"remark-math": "^5.1.1",
		"remix-auth": "^3.3.0",
		"remix-auth-form": "^1.1.2",
		"remix-auth-oauth2": "^1.3.0",
		"remix-utils": "^3.3.0"
	},
	"devDependencies": {
		"@iconify/json": "^2.1.121",
		"@julr/unocss-preset-heropatterns": "^0.0.2",
		"@kidonng/daisyui": "^2.31.0",
		"@remix-run/dev": "^1.7.2",
		"@remix-run/node": "^1.7.2",
		"@types/cookie-signature": "^1.0.4",
		"@unocss/cli": "^0.45.29",
		"@unocss/preset-attributify": "^0.45.29",
		"@unocss/transformer-directives": "^0.45.29",
		"nodemon": "^2.0.20",
		"prisma": "^4.4.0",
		"prop-types": "^15.8.1",
		"typescript": "^4.8.4",
		"unocss": "^0.45.29",
		"unocss-preset-daisy": "^1.2.0",
		"unocss-preset-scalpel": "^1.2.5",
		"unocss-preset-scrollbar": "^0.2.1",
		"vite": "^3.1.8"
	},
	"engines": {
		"node": ">=16"
	},
	"peerDependencies": {
		"@remix-run/node": "^1.7.2",
		"prop-types": "^15.8.1",
		"vite": "^3.1.6"
	}
}
