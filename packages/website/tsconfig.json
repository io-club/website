{
  "extends": [
    "astro/tsconfigs/strictest",
    "../../tsconfig.base.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@lib/*": [
        "./src/lib/*"
      ]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}