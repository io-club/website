{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2020", "esnext.asynciterable"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "ES2020",
